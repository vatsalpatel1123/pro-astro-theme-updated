---
// src/pages/about.astro
import Navigation from '../components/Navigation.astro';
import Footer from '../components/Footer.astro';
import AboutForm from '../components/aboutForm.astro';
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>About Us | Long Island Home Buyers - Built on Trust. Rooted in Long Island.</title>
    <script src="https://cdn.tailwindcss.com"></script>
    <style>
        @import url('https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700;800&display=swap');
        
        * {
            font-family: 'Inter', sans-serif;
        }
        
        .gradient-text {
            background: linear-gradient(135deg, #1e40af 0%, #3b82f6 100%);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
        }
        
        .hover-scale {
            transition: transform 0.3s ease;
        }
        
        .hover-scale:hover {
            transform: scale(1.05);
        }
        
        .glass-effect {
            background: rgba(255, 255, 255, 0.95);
            backdrop-filter: blur(10px);
        }

        .hero-pattern {
            background-image: 
                linear-gradient(to right, #f1f5f9 1px, transparent 1px),
                linear-gradient(to bottom, #f1f5f9 1px, transparent 1px);
            background-size: 40px 40px;
        }
    </style>
</head>
<body class="bg-slate-50">
    <!-- Navigation -->
    <Navigation />
    <!-- Hero Section -->
    <section class="relative bg-gradient-to-br from-slate-50 via-blue-50 to-slate-100 hero-pattern py-20 md:py-32">
        <div class="max-w-7xl mx-auto px-6">
            <div class="text-center max-w-4xl mx-auto">
                <div class="inline-block mb-6">
                    <span class="bg-blue-100 text-blue-800 px-4 py-2 rounded-full text-sm font-semibold">
                        40+ Years of Combined Experience
                    </span>
                </div>
                <h1 class="text-5xl md:text-6xl font-bold text-slate-900 mb-6">
                    Built on Trust. <br/>
                    <span class="gradient-text">Rooted in Long Island.</span>
                </h1>
                <p class="text-xl text-slate-600 mb-10 leading-relaxed max-w-3xl mx-auto">
                    For over 40 years, Long Island Home Buyers has been helping Long Island homeowners sell 
                    privately and confidently—with fair offers and local expertise.
                </p>
                <div class="flex flex-col sm:flex-row gap-4 justify-center mt-8">
                    <a href="#contact" class="bg-blue-600 text-white px-8 py-4 rounded-full hover:bg-blue-700 transition font-semibold text-lg shadow-lg hover:shadow-xl">
                        Get My Offer
                    </a>
                    <a href="tel:7182887300" class="bg-white text-slate-900 px-8 py-4 rounded-full hover:bg-slate-50 transition font-semibold text-lg border-2 border-slate-200 hover:border-blue-600">
                        Call (718) 288-7300
                    </a>
                </div>
                <p class="text-sm text-slate-500 mt-8">
                    ✓ No Obligations  ✓ Fair Cash Offers  ✓ Close on Your Timeline
                </p>
            </div>
        </div>
    </section>

    <!-- Our Story Section -->
    <section class="py-20 bg-white">
        <div class="max-w-7xl mx-auto px-6">
            <div class="grid md:grid-cols-2 gap-16 items-center">
                <div>
                    <span class="text-blue-600 font-semibold text-sm uppercase tracking-wide mb-4 block">Our Story</span>
                    <h2 class="text-4xl font-bold text-slate-900 mb-6">
                        Four Decades of Long Island Real Estate Excellence
                    </h2>
                    <div class="space-y-4 text-slate-600 text-lg leading-relaxed">
                        <p>
                            Long Island Home Buyers was founded on the principles of integrity, transparency, and 
                            local expertise. With over 40 years of combined experience in building and 
                            real estate development, we understand what makes Long Island properties valuable.
                        </p>
                        <p>
                            Our roots run deep in Manhasset, where we've witnessed the evolution of Long Island's 
                            real estate market firsthand. This intimate knowledge allows us to make fair, 
                            development-based offers that reflect true market value—not opportunistic lowballs.
                        </p>
                        <p>
                            We're not just cash buyers—we're builders, developers, and Long Island residents who 
                            care about our community. When you work with us, you're working with neighbors who 
                            understand your property's potential and respect your need for a smooth, stress-free sale.
                        </p>
                    </div>
                    <div class="mt-8 grid grid-cols-3 gap-6">
                        <div class="text-center">
                            <p class="text-4xl font-bold text-blue-600">40+</p>
                            <p class="text-sm text-slate-600 mt-2">Years Experience</p>
                        </div>
                        <div class="text-center">
                            <p class="text-4xl font-bold text-blue-600">500+</p>
                            <p class="text-sm text-slate-600 mt-2">Homes Purchased</p>
                        </div>
                        <div class="text-center">
                            <p class="text-4xl font-bold text-blue-600">100%</p>
                            <p class="text-sm text-slate-600 mt-2">Local & Trusted</p>
                        </div>
                    </div>
                </div>
                <div class="relative">
                    <img src="https://images.unsplash.com/photo-1560518883-ce09059eeffa?w=800&h=600&fit=crop" 
                         alt="Long Island Neighborhood" 
                         class="rounded-2xl shadow-2xl"/>
                    <div class="absolute -bottom-6 -left-6 bg-white p-6 rounded-xl shadow-xl">
                        <div class="flex items-center space-x-4">
                            <div class="w-12 h-12 bg-blue-100 rounded-full flex items-center justify-center">
                                <svg class="w-6 h-6 text-blue-600" fill="currentColor" viewBox="0 0 20 20">
                                    <path fill-rule="evenodd" d="M6.267 3.455a3.066 3.066 0 001.745-.723 3.066 3.066 0 013.976 0 3.066 3.066 0 001.745.723 3.066 3.066 0 012.812 2.812c.051.643.304 1.254.723 1.745a3.066 3.066 0 010 3.976 3.066 3.066 0 00-.723 1.745 3.066 3.066 0 01-2.812 2.812 3.066 3.066 0 00-1.745.723 3.066 3.066 0 01-3.976 0 3.066 3.066 0 00-1.745-.723 3.066 3.066 0 01-2.812-2.812 3.066 3.066 0 00-.723-1.745 3.066 3.066 0 010-3.976 3.066 3.066 0 00.723-1.745 3.066 3.066 0 012.812-2.812zm7.44 5.252a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"/>
                                </svg>
                            </div>
                            <div>
                                <p class="font-bold text-slate-900">Trusted Since 1983</p>
                                <p class="text-sm text-slate-600">Manhasset, Long Island</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Why Long Island Trusts Us -->
    <section class="py-20 bg-slate-50">
        <div class="max-w-7xl mx-auto px-6">
            <div class="text-center mb-16">
                <h2 class="text-4xl font-bold text-slate-900 mb-4">
                    Why Long Island Homeowners Trust Us
                </h2>
                <p class="text-xl text-slate-600 max-w-2xl mx-auto">
                    We're not just another "we buy houses" company. We're your Long Island neighbors 
                    with decades of proven expertise.
                </p>
            </div>

            <div class="grid md:grid-cols-3 gap-8 mt-8">
                <!-- Card 1 -->
                <div class="bg-white rounded-2xl p-8 shadow-lg hover:shadow-xl transition border border-slate-100">
                    <div class="w-16 h-16 rounded-2xl flex items-center justify-center mb-6">
                        <svg class="w-8 h-8 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z"/>
                        </svg>
                    </div>
                    <h3 class="text-2xl font-bold text-slate-900 mb-4">40+ Years of Experience</h3>
                    <p class="text-slate-600 leading-relaxed">
                        Combined decades of expertise in construction, development, and real estate. 
                        We know Long Island properties inside and out, from foundation to roof.
                    </p>
                </div>

                <!-- Card 2 -->
                <div class="bg-white rounded-2xl p-8 shadow-lg hover:shadow-xl transition border border-slate-100">
                    <div class="w-16 h-16 rounded-2xl flex items-center justify-center mb-6">
                        <svg class="w-8 h-8 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4"/>
                        </svg>
                    </div>
                    <h3 class="text-2xl font-bold text-slate-900 mb-4">Development-Based Offers</h3>
                    <p class="text-slate-600 leading-relaxed">
                        Our offers are rooted in real development analysis—not quick flips. We evaluate 
                        what your property can become, ensuring you receive its true worth.
                    </p>
                </div>

                <!-- Card 3 -->
                <div class="bg-white rounded-2xl p-8 shadow-lg hover:shadow-xl transition border border-slate-100">
                    <div class="w-16 h-16 rounded-2xl flex items-center justify-center mb-6">
                        <svg class="w-8 h-8 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z"/>
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z"/>
                        </svg>
                    </div>
                    <h3 class="text-2xl font-bold text-slate-900 mb-4">Local Knowledge, Fair Process</h3>
                    <p class="text-slate-600 leading-relaxed">
                        As Manhasset natives, we understand neighborhood dynamics, zoning, and market trends. 
                        Our transparent process means no hidden fees or surprises.
                    </p>
                </div>
            </div>
        </div>
    </section>

    <!-- Mission & Values -->
    <section class="py-20 bg-white">
        <div class="max-w-7xl mx-auto px-6">
            <div class="max-w-4xl mx-auto text-center">
                <span class="text-blue-600 font-semibold text-sm uppercase tracking-wide mb-4 block">Our Mission</span>
                <h2 class="text-4xl md:text-5xl font-bold text-slate-900 mb-8">
                    "We live here. We build here. <br/>
                    <span class="gradient-text">We buy here.</span>"
                </h2>
                <p class="text-xl text-slate-600 leading-relaxed mb-12">
                    Our mission is simple: to provide Long Island homeowners with a dignified, fair, and 
                    transparent alternative to traditional home sales. We believe in treating every property 
                    and every seller with the respect they deserve.
                </p>
            </div>

            <div class="grid md:grid-cols-3 gap-8 mt-16">
                <div class="text-center">
                    <div class="w-20 h-20 bg-blue-50 rounded-full flex items-center justify-center mx-auto mb-6">
                        <svg class="w-10 h-10 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 12a3 3 0 11-6 0 3 3 0 016 0z"/>
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M2.458 12C3.732 7.943 7.523 5 12 5c4.478 0 8.268 2.943 9.542 7-1.274 4.057-5.064 7-9.542 7-4.477 0-8.268-2.943-9.542-7z"/>
                        </svg>
                    </div>
                    <h3 class="text-xl font-bold text-slate-900 mb-3">Transparency</h3>
                    <p class="text-slate-600">
                        No hidden agendas. We explain our offers clearly and walk you through every step 
                        of the process with complete honesty.
                    </p>
                </div>

                <div class="text-center">
                    <div class="w-20 h-20 bg-blue-50 rounded-full flex items-center justify-center mx-auto mb-6">
                        <svg class="w-10 h-10 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z"/>
                        </svg>
                    </div>
                    <h3 class="text-xl font-bold text-slate-900 mb-3">Flexibility</h3>
                    <p class="text-slate-600">
                        Your timeline is our timeline. Whether you need to close in 7 days or 7 months, 
                        we accommodate your schedule.
                    </p>
                </div>

                <div class="text-center">
                    <div class="w-20 h-20 bg-blue-50 rounded-full flex items-center justify-center mx-auto mb-6">
                        <svg class="w-10 h-10 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z"/>
                        </svg>
                    </div>
                    <h3 class="text-xl font-bold text-slate-900 mb-3">Integrity</h3>
                    <p class="text-slate-600">
                        We stand by our word. Our reputation was built on fair dealing and maintaining 
                        the trust of Long Island families.
                    </p>
                </div>
            </div>
        </div>
    </section>

    <!-- Local Presence Section -->
    <section class="py-20 bg-gradient-to-br from-slate-50 to-blue-50">
        <div class="max-w-7xl mx-auto px-6">
            <div class="text-center mb-16">
                <h2 class="text-4xl font-bold text-slate-900 mb-4">
                    Proudly Serving Long Island Communities
                </h2>
                <p class="text-xl text-slate-600 max-w-2xl mx-auto">
                    From the North Shore to the South Shore, we know every neighborhood like the back of our hand.
                </p>
            </div>

            <div class="grid md:grid-cols-2 gap-12 items-center">
                <div>
                    <div class="bg-white rounded-2xl p-8 shadow-lg">
                        <h3 class="text-2xl font-bold text-slate-900 mb-6">Areas We Serve</h3>
                        <div class="grid grid-cols-2 gap-4">
                            <div class="flex items-center space-x-2">
                                <svg class="w-5 h-5 text-blue-600" fill="currentColor" viewBox="0 0 20 20">
                                    <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"/>
                                </svg>
                                <span class="text-slate-700 font-medium">Hicksville</span>
                            </div>
                            <div class="flex items-center space-x-2">
                                <svg class="w-5 h-5 text-blue-600" fill="currentColor" viewBox="0 0 20 20">
                                    <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"/>
                                </svg>
                                <span class="text-slate-700 font-medium">& More...</span>
                            </div>
                        </div>
                        <p class="mt-6 text-slate-600 text-sm">
                            We cover all of Nassau County and select areas of Queens and Suffolk County. 
                            Call us to confirm service in your area.
                        </p>
                    </div>
                </div>

                <div class="relative">
                    <img src="https://images.unsplash.com/photo-1524813686514-a57563d77965?w=800&h=600&fit=crop" 
                         alt="Long Island Map" 
                         class="rounded-2xl shadow-2xl"/>
                    <div class="absolute inset-0 bg-gradient-to-t from-blue-900/50 to-transparent rounded-2xl"></div>
                    <div class="absolute bottom-8 left-8 right-8 text-white">
                        <h4 class="text-2xl font-bold mb-2">Nassau County & Beyond</h4>
                        <p class="text-blue-100">Serving homeowners across Long Island with pride since 1983</p>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Team Section -->
    <section class="py-20 bg-white">
        <div class="max-w-7xl mx-auto px-6">
            <div class="text-center mb-16">
                <span class="text-blue-600 font-semibold text-sm uppercase tracking-wide mb-4 block">Meet the Team</span>
                <h2 class="text-4xl font-bold text-slate-900 mb-4">
                    Experienced Professionals You Can Trust
                </h2>
                <p class="text-xl text-slate-600 max-w-2xl mx-auto">
                    Our team combines decades of construction, development, and real estate expertise.
                </p>
            </div>

            <div class="grid md:grid-cols-3 gap-8 mt-8">
                <div class="text-center group">
                    <div class="relative mb-6 overflow-hidden rounded-2xl">
                        <img src="https://images.unsplash.com/photo-1560250097-0b93528c311a?w=400&h=500&fit=crop" 
                             alt="Team Member" 
                             class="w-full h-80 object-cover transform group-hover:scale-105 transition duration-500"/>
                        <div class="absolute inset-0 bg-gradient-to-t from-slate-900/70 to-transparent"></div>
                        <div class="absolute bottom-6 left-6 right-6 text-white">
                            <h3 class="text-xl font-bold mb-1">Michael Harrison</h3>
                            <p class="text-blue-200 text-sm">Founder & Lead Developer</p>
                        </div>
                    </div>
                    <p class="text-slate-600">
                        25+ years in commercial and residential development across Long Island
                    </p>
                </div>

                <div class="text-center group">
                    <div class="relative mb-6 overflow-hidden rounded-2xl">
                        <img src="https://images.unsplash.com/photo-1573496359142-b8d87734a5a2?w=400&h=500&fit=crop" 
                             alt="Team Member" 
                             class="w-full h-80 object-cover transform group-hover:scale-105 transition duration-500"/>
                        <div class="absolute inset-0 bg-gradient-to-t from-slate-900/70 to-transparent"></div>
                        <div class="absolute bottom-6 left-6 right-6 text-white">
                            <h3 class="text-xl font-bold mb-1">Sarah Chen</h3>
                            <p class="text-blue-200 text-sm">Real Estate Director</p>
                        </div>
                    </div>
                    <p class="text-slate-600">
                        Licensed broker with deep knowledge of Nassau County property values
                    </p>
                </div>

                <div class="text-center group">
                    <div class="relative mb-6 overflow-hidden rounded-2xl">
                        <img src="https://images.unsplash.com/photo-1472099645785-5658abf4ff4e?w=400&h=500&fit=crop" 
                             alt="Team Member" 
                             class="w-full h-80 object-cover transform group-hover:scale-105 transition duration-500"/>
                        <div class="absolute inset-0 bg-gradient-to-t from-slate-900/70 to-transparent"></div>
                        <div class="absolute bottom-6 left-6 right-6 text-white">
                            <h3 class="text-xl font-bold mb-1">David Rodriguez</h3>
                            <p class="text-blue-200 text-sm">Construction Manager</p>
                        </div>
                    </div>
                    <p class="text-slate-600">
                        Expert in property assessment and renovation cost analysis
                    </p>
                </div>
            </div>
        </div>
    </section>

    <!-- Testimonials Section -->
    <section class="py-20 bg-slate-50">
        <div class="max-w-7xl mx-auto px-6">
            <div class="text-center mb-16">
                <h2 class="text-4xl font-bold text-slate-900 mb-4">
                    What Long Island Homeowners Say
                </h2>
                <p class="text-xl text-slate-600">
                    Real stories from real sellers in your neighborhood
                </p>
            </div>

            <div class="grid md:grid-cols-3 gap-8">
                <div class="bg-white rounded-2xl p-8 shadow-lg">
                    <div class="flex mb-4">
                        <svg class="w-5 h-5 text-yellow-400" fill="currentColor" viewBox="0 0 20 20">
                            <path d="M9.049 2.927c.3-.921 1.603-.921 1.902 0l1.07 3.292a1 1 0 00.95.69h3.462c.969 0 1.371 1.24.588 1.81l-2.8 2.034a1 1 0 00-.364 1.118l1.07 3.292c.3.921-.755 1.688-1.54 1.118l-2.8-2.034a1 1 0 00-1.175 0l-2.8 2.034c-.784.57-1.838-.197-1.539-1.118l1.07-3.292a1 1 0 00-.364-1.118L2.98 8.72c-.783-.57-.38-1.81.588-1.81h3.461a1 1 0 00.951-.69l1.07-3.292z"/>
                        </svg>
                        <svg class="w-5 h-5 text-yellow-400" fill="currentColor" viewBox="0 0 20 20">
                            <path d="M9.049 2.927c.3-.921 1.603-.921 1.902 0l1.07 3.292a1 1 0 00.95.69h3.462c.969 0 1.371 1.24.588 1.81l-2.8 2.034a1 1 0 00-.364 1.118l1.07 3.292c.3.921-.755 1.688-1.54 1.118l-2.8-2.034a1 1 0 00-1.175 0l-2.8 2.034c-.784.57-1.838-.197-1.539-1.118l1.07-3.292a1 1 0 00-.364-1.118L2.98 8.72c-.783-.57-.38-1.81.588-1.81h3.461a1 1 0 00.951-.69l1.07-3.292z"/>
                        </svg>
                        <svg class="w-5 h-5 text-yellow-400" fill="currentColor" viewBox="0 0 20 20">
                            <path d="M9.049 2.927c.3-.921 1.603-.921 1.902 0l1.07 3.292a1 1 0 00.95.69h3.462c.969 0 1.371 1.24.588 1.81l-2.8 2.034a1 1 0 00-.364 1.118l1.07 3.292c.3.921-.755 1.688-1.54 1.118l-2.8-2.034a1 1 0 00-1.175 0l-2.8 2.034c-.784.57-1.838-.197-1.539-1.118l1.07-3.292a1 1 0 00-.364-1.118L2.98 8.72c-.783-.57-.38-1.81.588-1.81h3.461a1 1 0 00.951-.69l1.07-3.292z"/>
                        </svg>
                        <svg class="w-5 h-5 text-yellow-400" fill="currentColor" viewBox="0 0 20 20">
                            <path d="M9.049 2.927c.3-.921 1.603-.921 1.902 0l1.07 3.292a1 1 0 00.95.69h3.462c.969 0 1.371 1.24.588 1.81l-2.8 2.034a1 1 0 00-.364 1.118l1.07 3.292c.3.921-.755 1.688-1.54 1.118l-2.8-2.034a1 1 0 00-1.175 0l-2.8 2.034c-.784.57-1.838-.197-1.539-1.118l1.07-3.292a1 1 0 00-.364-1.118L2.98 8.72c-.783-.57-.38-1.81.588-1.81h3.461a1 1 0 00.951-.69l1.07-3.292z"/>
